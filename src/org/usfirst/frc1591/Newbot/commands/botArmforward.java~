    // Called repeatedly when this Command is scheduled to run
    protected void execute() {
    	double Y = Robot.oi.armStick.getY();
    	double SPEED = SCALE*Robot.oi.armStick.getY();
 	   
    	if( ( (Y>0.05)&& !(Robot.arm.minHeight(0.2)) ) || ((Y<-0.05)&& !(Robot.arm.maxHeight(.80))))
    		Robot.arm.actuatorAction(SPEED);
    	
    	else
    		Robot.arm.armstop();
    }

